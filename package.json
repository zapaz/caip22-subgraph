{
  "name": "caip22",
  "version": "0.0.9",
  "license": "UNLICENSED",
  "scripts": {
    "which": "which graph",
    "version": "graph --version",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy-goerli": "graph deploy --node https://api.studio.thegraph.com/deploy/ caip22",
    "deploy-matic": "graph deploy --product hosted-service zapaz/caip22-matic",
    "all": "npm run codegen && npm run build && npm run deploy",
    "create-local": "graph create --node http://localhost:8020/ caip22",
    "remove-local": "graph remove --node http://localhost:8020/ caip22",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 caip22"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.35.0",
    "@graphprotocol/graph-ts": "0.29.0"
  }
}